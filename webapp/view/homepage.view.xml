<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" xmlns:tnt="sap.tnt" controllerName="test1.controller.homepage">
	<App id="app">
		<pages>
			<Page title=" Welcome to Dr.thinking" class="text1">
				<content>
					<tnt:ToolHeader>
						<Button icon="sap-icon://menu2" type="Transparent">
							<layoutData>
								<OverflowToolbarLayoutData priority="NeverOverflow"/>
							</layoutData>
						</Button>
						<ToolbarSpacer/>
						<core:Icon src="sap-icon://activate"/>
						<Text text="welcome" wrapping="false">
							<layoutData>
								<OverflowToolbarLayoutData priority="Disappear"/>
							</layoutData>
						</Text>
						<ToolbarSpacer/>
						<Text text="Welcome" wrapping="false">
							<layoutData>
								<OverflowToolbarLayoutData priority="Disappear"/>
							</layoutData>
						</Text>
					</tnt:ToolHeader>
					<VBox fitContainer="true" justifyContent="Center" alignItems="Center" alignContent="Center">
						<items>
							<Label text="welcome back Daniel Thinking! what would you like to do today" class="text2"/>
						</items>
					</VBox>
					
					
				
					
					
					<TileContainer id="container1" tileDelete="handleTileDelete">
						<StandardTile icon="sap-icon://menu2" title="View and manage your appointments"/>
					</TileContainer>
					
					<TileContainer id="container2" tileDelete="handleTileDelete">
						<StandardTile icon="sap-icon://menu2" title="View and change your profile"/>
					</TileContainer>
					
					
					<TileContainer id="container3" tileDelete="handleTileDelete">
						<StandardTile icon="sap-icon://menu2" title="search for available patients"/>
					</TileContainer>
					
					<TileContainer id="container4" tileDelete="handleTileDelete">
						<StandardTile icon="sap-icon://menu2" title="View chats"/>
					</TileContainer>
				
		
							<Label text="you have an appointment in 27mins" class="text2" />
							<Button text="please click here to access the chat room" type="Transparent"></Button>
					
				
					
					
				
					
				</content>
				<footer>
					<Bar>
						<contentLeft>
							<Text text="hi user"/>
						</contentLeft>
						<contentRight>
							<Text text="hi user"/>
						</contentRight>
					</Bar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>