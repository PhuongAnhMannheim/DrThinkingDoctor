<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" xmlns:tnt="sap.tnt" controllerName="test1.controller.profile">
	<App id="app">
		<pages>
			<Page title=" Welcome to Dr.thinking" class="text1">
				<content>
					<tnt:ToolHeader>
						<Button icon="sap-icon://menu2" type="Transparent">
							<layoutData>
								<OverflowToolbarLayoutData priority="NeverOverflow"/>
							</layoutData>
						</Button>
						<ToolbarSpacer/>
						<core:Icon src="sap-icon://activate"/>
						<Text text="Feel free to view your profile" wrapping="false">
							<layoutData>
								<OverflowToolbarLayoutData priority="Disappear"/>
							</layoutData>
						</Text>
						<ToolbarSpacer/>
						<Text text="Daniel Thinking" wrapping="false">
							<layoutData>
								<OverflowToolbarLayoutData priority="Disappear"/>
							</layoutData>
						</Text>
					</tnt:ToolHeader>
					
					<ObjectHeader responsive="true" intro="Dr.think" title="Dr.thinking" number="shcbc">
						<attributes>
			<ObjectAttribute
				title="Manufacturer"
				text="dhkbv"/>
		</attributes>
					</ObjectHeader>
					
					<l:VerticalLayout class="sapUiContentPadding equalColumns" width="100%">
						<l:content>
							<FlexBox width="95%" class="columns" alignItems="Stretch">
								<items>
									<List binding="{/doc_info/0}" headerText="{Name}">
										<DisplayListItem label="Name" value="{Name}"/>
										<DisplayListItem label="Age" value="{age}"/>
									</List>
									<List binding="{/doc_info/0}" headerText="{Name}">
										<DisplayListItem label="Name" value="{Name}"/>
										<DisplayListItem label="Age" value="{age}"/>
									</List>
								</items>
							</FlexBox>
						</l:content>
					</l:VerticalLayout>
					<l:VerticalLayout class="sapUiContentPadding equalColumns" width="100%">
						<l:content>
							<FlexBox width="95%" class="columns" alignItems="Stretch">
								<items>
									<List binding="{/doc_info/0}" headerText="personal data">
										<DisplayListItem label="Name" value="{Name}"/>
										<DisplayListItem label="Age" value="{age}"/>
									</List>
									<List binding="{/doc_info/0}" headerText="Medical experience data">
										<DisplayListItem label="Name" value="{Name}"/>
										<DisplayListItem label="Age" value="{age}"/>
									</List>
								</items>
							</FlexBox>
						</l:content>
					</l:VerticalLayout>
					<l:VerticalLayout class="sapUiContentPadding equalColumns" width="100%">
						<l:content>
							<FlexBox width="95%" class="columns" alignItems="Stretch">
								<items>
									<List binding="{/doc_info/0}" headerText="Certificate and licenses">
										<DisplayListItem label="Name" value="{Name}"/>
										<DisplayListItem label="Age" value="{age}"/>
									</List>
									<List binding="{/doc_info/0}" headerText="biO">
										<DisplayListItem label="Name" value="{Name}"/>
										<DisplayListItem label="Age" value="{age}"/>
									</List>
								</items>
							</FlexBox>
						</l:content>
					</l:VerticalLayout>
				</content>
				<footer>
					<Bar>
						<contentLeft>
							<Text text="hi user"/>
						</contentLeft>
						<contentRight>
							<Text text="hi user"/>
						</contentRight>
					</Bar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>