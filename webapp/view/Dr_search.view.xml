<mvc:View xmlns:l="sap.ui.layout" xmlns:tnt="sap.tnt" xmlns:u="sap.ui.unified" class="viewPadding" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
controllerName="DrThinkingDoctor.controller.Dr">

	<Page>
		<content>
			<SearchField width="90%" class="sapUiSmallMargin" placeholder="search for..."/>
			<l:VerticalLayout class="sapUiContentPadding">
				<ImageContent
					class="sapUiLargeMarginTop sapUiLargeMarginBottom"
					src="/webapp/resources/dr_thinking_logo.png"
					description="Icon"
					press="press">
				</ImageContent>
				
				<Toolbar>
					<Button text="Search" press="onPress" />
				</Toolbar>
			</l:VerticalLayout>
		</content>

	</Page>

	
</mvc:View>