<mvc:View xmlns:l="sap.ui.layout" xmlns:tnt="sap.tnt" xmlns:u="sap.ui.unified" class="viewPadding" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="test1.controller.Reg_p">
	<Page title=" Welcome to Dr.thinking" class="text1">
			<tnt:ToolHeader>
						
						<ToolbarSpacer/>
						<core:Icon src="sap-icon://activate"/>
						<Text text="Enter your credential to register" wrapping="false">
							<layoutData>
								<OverflowToolbarLayoutData priority="Disappear"/>
							</layoutData>
						</Text>
						<ToolbarSpacer/>
						
					</tnt:ToolHeader>
					
		<List headerText="Patient Registeration">
			<InputListItem label="Email">
				<Input placeholder="Email" required="true" width="250px"/>
			</InputListItem>
			<InputListItem label="Password">
				<Input placeholder="Password" required="true" width="250px"/>
			</InputListItem>
			<InputListItem label="Re-enter Password">
				<Input placeholder="Password" required="true" width="250px"/>
			</InputListItem>
			<InputListItem label="Date of birth">
				<Input placeholder="DD.MM.YYYY" required="true" width="250px"/>
			</InputListItem>
			<InputListItem label="Last Name">
				<Input placeholder="Last Name" required="true" width="250px"/>
			</InputListItem>
			<InputListItem label="First Name">
				<Input placeholder="First Name" required="true" width="250px"/>
			</InputListItem>
			<InputListItem label="Insurance No">
				<Input placeholder="Insurance No" required="true" width="250px"/>
			</InputListItem>
			<InputListItem label="IBAN">
				<Input placeholder="IBAN" required="true" width="250px"/>
			</InputListItem>
			<InputListItem label="BIC">
				<Input placeholder="BIC" required="true" width="250px"/>
			</InputListItem>
		</List>
        <Text text="We recommend that you maintain your personal health information in your profile page to improve diagnsis efficiency  " wrapping="false">
		</Text>
		<Button text="register" type="Emphasized"/>
			<footer>
				<Bar>
				<contentLeft>
				<Text text="hi user"/>
				</contentLeft>
				<contentRight>
				<Text text="hi user"/>
				</contentRight>
				</Bar>
				</footer>
	</Page>
	</mvc:View>