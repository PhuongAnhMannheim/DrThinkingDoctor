<mvc:View controllerName="DrThinkingDoctor.controller.Home" 
	xmlns:html="http://www.w3.org/1999/xhtml" 
	xmlns:mvc="sap.ui.core.mvc" 
	displayBlock="true" 
	xmlns="sap.m">
	<App>
		<pages><Page title="{i18n>title}">
				<content>
					<Button text="Search for Available Patients" press="onPressSearch"></Button>	
					<Button text="Appointments" press="onPressAppointments"></Button>
					<Button text="View and Maintain Profile" press="onPressProfile"></Button>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>